{
  "name": "goats-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test-env-dev": "mocha --require test/setup.js --exit",
    "test": "env NODE_ENV=test mocha --require test/setup.js --exit",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "seed-country": "psql -U goats_api -f ./seeds/001.seed.country.sql",
    "seed-schema": "psql -U goats_api -f ./seeds/002.seed.schema.sql",
    "seed-schema:production": "heroku pg:psql -f ./seeds/002.seed.schema.sql",
    "predeploy": "npm audit && npm run migrate:production",
    "deploy": "git push heroku master",
    "pug-pretty": "pug --pretty src/html",
    "insert-sql": "psql -U goats_api -f ./seeds/003.seed.country_lat_lng.sql",
    "insert-sql:production": "heroku pg:psql -f ./seeds/003.seed.country_lat_lng.sql"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.2.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.22.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.13",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "fb": "^2.0.0",
    "flash": "^1.1.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^4.0.0",
    "pg": "^8.2.1",
    "pug": "^3.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "sendgrid": "^5.2.3",
    "serve-favicon": "^2.5.0",
    "session": "^0.1.0",
    "uuid": "^8.2.0",
    "winston": "^3.3.3",
    "xss": "^1.0.7"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-date-string": "^0.1.0",
    "chai-json-pattern": "^1.1.0",
    "chai-uuid": "^1.0.6",
    "eslint": "^7.4.0",
    "mocha": "^8.0.1",
    "postgrator-cli": "^4.0.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "v10.15.3"
  }
}
