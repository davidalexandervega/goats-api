{
  "name": "goats-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test-env-dev": "mocha --require test/setup.js --exit",
    "test": "env NODE_ENV=test mocha --require test/setup.js --exit",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:production": "env SSL=true DATABASE_URL=\"$(dotenv cross-var heroku config:get DATABASE_URL -a %HEROKU_APP_NAME%)\" npm run migrate",
    "seed-country": "dotenv cross-var \"psql -U postgres -d goats_api -v SEEDS_PATH=%SEEDS_PATH% -f ./seeds/001.seed.country.sql\"",
    "seed-latlng": "psql -U postgres -d goats_api -f ./seeds/002.seed.country_lat_lng.sql",
    "seed-adhoc:production": "heroku pg:psql -f ./seeds/002.seed.country_lat_lng.sql",
    "predeploy": "npm audit && npm run migrate:production",
    "deploy": "git push heroku master",
    "pug-pretty": "pug --pretty src/html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.0.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.19.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-var": "^1.1.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^5.0.0",
    "express": "^4.17.1",
    "express-form-data": "^2.0.11",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.1",
    "fb": "^2.0.0",
    "flash": "^1.1.0",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.4",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^3.3.0",
    "pg": "^8.7.3",
    "pg-native": "^3.0.0",
    "postgrator-cli": "^5.1.0",
    "pug": "^3.0.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.5",
    "sendgrid": "^4.2.0",
    "serve-favicon": "^2.5.0",
    "session": "^0.1.0",
    "uuid": "^3.3.3",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-date-string": "^0.1.0",
    "chai-json-pattern": "^1.1.0",
    "chai-uuid": "^1.0.6",
    "eslint": "^8.10.0",
    "mocha": "^9.2.2",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "17.6.0"
  }
}
